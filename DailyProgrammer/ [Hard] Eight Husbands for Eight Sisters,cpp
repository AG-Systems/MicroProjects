#include <iostream>

int main()
{
  bool A = false;
  bool B = false;
  bool C = false;
  bool X = false;
  bool Y = false;
  bool Z = false;
  
  int mA = 1;
  int mB = 2;
  int mC = 3;
  int fX;
  int fY;
  int fZ;
  
  if ( A = false)
  {
    int mA = int fY;
  }
  if ( A = true)
  {
    int mA = int fZ;
  }
  if ( B = false)
  {
    int mB = int fZ;
  }
  if ( B = true)
  {
    int mB = fX;
  }
  if ( C = false)
  {
    int mC = fX;
  }
  of ( C = false)
  {
    int mC = fY;
  }
  }
}
